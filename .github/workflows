name: Deploy to Canary
on: 
  pull_request:
    types: [labeled]

jobs:
  build:
    name: Build
    runs-on: ubuntu-latest
    steps:
    - name: Build the Docker container image
      env: 
        TEST: "foo"
        TEST2: $TEST/test
      run: |
        echo "hello world" 
        echo $TEST2


 
